        "ExpiresIn": 1,
        "LongTermExpiresIn": 360,
        "Audience": "https://audience.spriggan.com.br",
        "Issuer": "https://issuer.spriggan.com.br",
        "IssuerKey": null,
        "RolesWithLongTermExpiresIn": []
      }
