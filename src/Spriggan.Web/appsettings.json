{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "ConnectionStrings": {
    "Main": "",
    "Identity": ""
  },
  "Core": {
    "Protection": {
      "Name": "Default",
      "Path": null
    },
    "Web": {
      "Culture": {
        "Default": "pt-BR",
        "Supported": ["pt-BR"]
      },
      "RateLimiter": {
